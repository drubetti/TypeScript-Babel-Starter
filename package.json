{
  "name": "typescript-babel-starter",
  "author": "Daniele Rubetti",
  "description": "An example project with TypeScript and Babel",
  "version": "0.0.19",
  "license": "MIT",
  "repository": "github:drubetti/TypeScript-Babel-Starter",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:js": "babel src --out-dir dist --extensions \".ts\"",
    "build:types": "tsc --emitDeclarationOnly",
    "postinstall": "husky install",
    "prepare": "husky install",
    "start": "npm run build:js && node dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.11.18",
    "babel-core": "^7.0.0-bridge.0",
    "husky": "^8.0.3",
    "prettier": "2.8.3",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "dotenv": "^16.0.3",
    "source-map-support": "^0.5.21"
  }
}
