{
  "name": "typescript-babel-starter",
  "author": "Daniele Rubetti",
  "description": "An example project with TypeScript and Babel",
  "version": "0.0.24",
  "license": "MIT",
  "repository": "github:drubetti/TypeScript-Babel-Starter",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:js": "babel src --out-dir dist --extensions \".ts\"",
    "build:types": "tsc --emitDeclarationOnly",
    "postinstall": "husky install",
    "prepare": "husky install",
    "start": "npm run build:js && node dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/plugin-syntax-import-assertions": "^7.23.3",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.10.5",
    "babel-core": "^7.0.0-bridge.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.6",
    "dotenv": "^16.3.1",
    "source-map-support": "^0.5.21"
  }
}
